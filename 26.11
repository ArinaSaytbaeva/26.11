//74
#include <bits/stdc++.h>  
using namespace std;  
#include <iostream> 
 
int main() { 
    int n; 
 
    cout << "Введите возраст (натуральное число, n <= 100): "; 
    cin >> n; 
 
    if (n < 1 || n > 100) { 
        cout << "Ошибка: возраст должен быть натуральным числом от 1 до 100." << std::endl; 
        return 1; 
    } 
 
    // Определение правильного склонения слова "год" 
    if (n % 10 == 1 && n % 100 != 11) { 
        cout << n << " год" << endl; 
    } else if ((n % 10 >= 2 && n % 10 <= 4) && (n % 100 < 12 || n % 100 > 14)) { 
        cout << n << " года" << endl; 
    } else { 
        cout << n << " лет" << endl; 
    } 
 
    return 0; 
}
